(function(){var f=YAHOO.util,e=f.Dom,d=f.Event;var a={};var b={cateCost1:"publish_detect=cate_01",cateCost2:"publish_detect=cate_02",cateCost3:"publish_detect=cate_03",cateCost4:"publish_detect=cate_04",cateCost5:"publish_detect=cate_05",uploadCost1:"publish_detect=upload_01",uploadCost2:"publish_detect=upload_02",uploadCost3:"publish_detect=upload_03",uploadCost4:"publish_detect=upload_04",uploadCost5:"publish_detect=upload_05",searchCat:"publish_detect=app_01",oftenUse:"publish_detect=app_02",reEditCat:"publish_detect=app_03",catePath:"publish_detect=app_04",suc_view:"publish_app=app_01",suc_onsale:"publish_app=app_02",suc_publish:"publish_app=app_03",share_tjh:"publish_app=app_04",share_weibo:"publish_app=app_05",share_renren:"publish_app=app_06",share_kaixin:"publish_app=app_07",share_copy:"publish_app=app_08"};function c(h){if(!h){return}var i="HOLD"+new Date().toString();var g=window[i]=new Image(1,1);d.on(g,"error load",function(){window[i]=null});g.src="http://www.atpanel.com/jsclick?cache="+(Math.floor(Math.random()*9999999)+1)+"&"+h}TB.Sell=TB.Sell||{};TB.Sell.Detect={timer:{start:new Date().getTime(),getDomReadyLevel:function(){var g=new Date().getTime()-this.start;if(g<=2000){return 1}else{if(g>2000&&g<=5000){return 2}else{if(g>5000&&g<=8000){return 3}else{if(g>8000&&g<=15000){return 4}else{return 5}}}}},getPublishCost:function(){var g=new Date().getTime()-this.start;if(g<=10000){return 1}else{if(g>10000&&g<=20000){return 2}else{if(g>20000&&g<=30000){return 3}else{if(g>30000&&g<=120000){return 4}else{return 5}}}}}},use:function(g){if(!a[g]){a[g]=true;c(b[g])}},init:function(){d.onDOMReady(function(){var g=e.getElementsByClassName("J_DetectTrigger");d.on(g,"mousedown",function(h){if(["a","select","input","button","img"].indexOf(d.getTarget(h).tagName.toLowerCase())===-1){return}app=this.getAttribute("data-detect");if(app){TB.Sell.Detect.use(app)}});d.on("J_CatePubBtn","click",function(){try{TB.Sell.Detect.use("cateCost"+TB.Sell.Detect.timer.getPublishCost())}catch(h){}});d.on("J_MainForm","submit",function(){try{TB.Sell.Detect.use("uploadCost"+TB.Sell.Detect.timer.getPublishCost())}catch(h){}})})}};TB.Sell.Detect.init()})();