KISSY.add("userVerify/mod/widget/dialog/common",function(e,t,n,i){var o={},r=t.all,a={title:"提示",isNeedDrag:!0,closable:!0,closeAction:"destroy",mask:!0,effect:{effect:"fade",duration:.2},align:{points:["cc","cc"]},elCls:"common",zIndex:999,width:400};return e.mix(o,{parseData:function(t,n){var i,o,r=e.clone(a);return e.isPlainObject(t)?i=e.mix(r,t):e.isFunction(t)?(i=r,n=t):(o=n,i=r),e.isFunction(n)&&(o=n),{config:i,callback:o}},getDialog:function(e){return new n.Dialog(e)},setDragAble:function(e){var t,n=r(e.get("el")),i=this;t=i._initDD(n,[".ks-stdmod-header"])},_initDD:function(e,t){return new i.Draggable({node:e,cursor:"move",move:!0,handlers:t})},setContent:function(t,n,i){return t.set("bodyContent",e.substitute(n,i)),t.set("headerContent",i.title),this}}),o},{requires:["node","overlay","dd"]});